algoritmo "Leitura de dados"

var x: caractere
    y: inteiro

inicio
    leia(x) //o "usuário" que vai inserir a informação (x), no caso o seu usuário
    escreval("Nome de usuário:", x)
    leia(y)
    escreval("Sua idade é ", y)
    //auto-explicativo, o programa vai ler e escrever a informação que lhe foi passada

fimalgoritmo