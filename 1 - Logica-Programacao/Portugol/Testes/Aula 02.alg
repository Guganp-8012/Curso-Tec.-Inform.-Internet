algoritmo "Leitura de dados"

var a: caractere
    b: inteiro
    c: caractere

inicio
       escreva("Nome? ")
    leia(a)
       escreval(a, " ")
       escreva("Idade? ")
    leia(b)
       escreval(b, " ")
       escreva("Email? ")
    leia(c)
       escreval(c, " ")
       escreval(" ")
    escreva("  Seja bem-vindo ", a, ", obrigado por escolher nossos serviços! (°o°)")

fimalgoritmo